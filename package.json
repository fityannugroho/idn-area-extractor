{
  "name": "idn-area-extractor",
  "version": "0.4.0",
  "description": "Extract Indonesia area data from the raw sources to CSV",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "idnxtr": "lib/cli.js"
  },
  "files": [
    "lib",
    "data"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "build": "tsup src/index.ts src/cli.ts --format esm -d lib --clean --dts src/index.ts --sourcemap --minify",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fityannugroho/idn-area-extractor.git"
  },
  "keywords": [
    "extractor",
    "csv",
    "idn-area",
    "indonesia",
    "wilayah"
  ],
  "author": "fityan <fityannugroho@gmail.com> (https://github.com/fityannugroho)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fityannugroho/idn-area-extractor/issues"
  },
  "homepage": "https://github.com/fityannugroho/idn-area-extractor#readme",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@types/papaparse": "^5.3.8",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitest/coverage-v8": "^0.34.3",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "@inquirer/prompts": "^3.0.4",
    "file-type": "^18.5.0",
    "idn-area-data": "^3.0.0",
    "jest-diff": "^29.6.4",
    "meow": "^12.1.1",
    "multi-integer-range": "^5.2.0",
    "ora": "^7.0.1",
    "papaparse": "^5.4.1",
    "pdfjs-dist": "^4.0.269",
    "super-regex": "^0.3.0"
  }
}
